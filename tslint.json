{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
      "quotemark": [true, "single", "jsx-double"],
      "ordered-imports": [
        false,
        {
          "import-sources-order": "case-insensitive",
          "named-imports-order": "case-insensitive"
        }
      ],
      "interface-name" : [true, "never-prefix"]
    },
    "rulesDirectory": []
}
